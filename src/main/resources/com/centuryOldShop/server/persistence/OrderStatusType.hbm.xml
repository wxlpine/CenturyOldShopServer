<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.centuryOldShop.server.persistence" auto-import="true">
    <class name="OrderStatusType" mutable="true" dynamic-update="false" dynamic-insert="false"
           select-before-update="false" lazy="true" abstract="false">
        <id name="orderStatusType">
            <column name="orderStatusType" sql-type="nvarchar(255)" not-null="true" length="255"/>
        </id>
        <bag name="orderForm" outer-join="false" lazy="true" optimistic-lock="true">
            <key>
                <column name="orderStatusType" sql-type="nvarchar(255)" not-null="true" length="255"/>
            </key>

            <one-to-many class="com.centuryOldShop.server.persistence.OrderForm"/>
        </bag>
    </class>
</hibernate-mapping>